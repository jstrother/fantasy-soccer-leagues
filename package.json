{
  "name": "fantasy-soccer-leagues",
  "version": "1.0.0",
  "description": "A fantasy soccer league that auto-updates via api calls",
  "main": "server.js",
  "scripts": {
    "test:server": "mocha test/test-server/test-index.js",
    "test:client": "jest || true",
    "test:initial": "npm run test:server && npm run test:client",
    "test:watch:server": "onchange \"./test/test-server\" -- npm run test:server",
    "test:watch:client": "onchange \"./test/test-client\" -- npm run test:client",
    "testing:watch:server": "onchange \"./server\" -- npm run test:server",
    "testing:watch:pFunctions": "onchange \"./server/programFunctions\" -- npm run test:server",
    "testing:watch:components": "onchange \"./components\" -- npm run test:client",
    "testing:watch:flow": "onchange \"./flow\" -- npm run test:client",
    "test:watch": "concurrently \"npm run test:watch:server\" \"npm run test:watch:client\" \"npm run testing:watch:server\" \"npm run testing:watch:pFunctions\" \"npm run testing:watch:components\" \"npm run testing:watch:flow\"",
    "test": "npm run test:initial && npm run test:watch",
    "scss": "node-sass --output-style compressed scss/styles.scss public/styles.css",
    "autoprefixer": "postcss -u autoprefixer -r public/styles.css",
    "cleancss": "cleancss -o public/styles.css public/styles.css",
    "image": "imagemin html/img/*.* -o public/img",
    "font": "cp html/font/*.* public/font/",
    "html": "cp html/*.* public/",
    "start": "nodemon --harmony server/server.js",
    "build:html": "npm run html && npm run image && npm run font",
    "build:css": "npm run scss && npm run autoprefixer && npm run cleancss",
    "build:js": "webpack",
    "build": "npm run build:html && npm run build:css && npm run build:js",
    "watch:html": "onchange \"./html\" -- npm run build:html",
    "watch:css": "onchange \"./scss\" -- npm run build:css",
    "watch:components": "onchange \"./components\" -- npm run build:js",
    "watch:flow": "onchange \"./flow\" -- npm run build:js",
    "watch:server": "onchange \"./server\" -- npm run build:js",
    "watch:pFunctions": "onchange \"./server/programFunctions\" -- npm run build:js",
    "watch:wConfig": "onchange \"./webpack.config.js\" -- npm run build:js",
    "watch": "concurrently \"npm run watch:html\" \"npm run watch:css\" \"npm run watch:components\" \"npm run watch:flow\" \"npm run watch:wConfig\" \"npm run watch:server\" \"npm run watch:pFunctions\" \"npm run start\""
  },
  "nodemonConfig": {
    "ignore": [
      "test/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jstrother/fantasy-soccer-leagues.git"
  },
  "keywords": [
    "fantasy",
    "soccer"
  ],
  "author": "Jim Strother",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jstrother/fantasy-soccer-leagues/issues"
  },
  "homepage": "https://github.com/jstrother/fantasy-soccer-leagues#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "clean-css": "^4.1.9",
    "clean-css-cli": "^4.1.10",
    "concurrently": "^3.5.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "imagemin": "^5.2.2",
    "imagemin-cli": "^3.0.0",
    "jest": "^20.0.4",
    "mocha": "^3.5.3",
    "node-sass": "^4.5.2",
    "nodemon": "^1.12.1",
    "normalize-scss": "^6.0.0",
    "onchange": "^3.2.1",
    "postcss-cli": "^4.1.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "redux-mock-store": "^1.3.0",
    "webpack": "^2.7.0"
  },
  "dependencies": {
    "S": "0.0.1",
    "body-parser": "^1.18.2",
    "expect": "^21.2.1",
    "express": "^4.16.2",
    "fetch-mock": "^5.13.1",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.4",
    "material-ui": "^0.17.4",
    "mongodb": "^2.2.33",
    "mongoose": "^4.12.2",
    "mquery": "^2.3.2",
    "nock": "^9.0.22",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "passport-http-bearer": "^1.0.1",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  }
}
