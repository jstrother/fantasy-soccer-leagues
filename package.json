{
  "name": "fantasy-soccer-leagues",
  "version": "1.0.0",
  "description": "A fantasy soccer league that auto-updates via web scraping",
  "main": "server.js",
  "scripts": {
    "test": "mocha test/test-index.js",
    "scss": "node-sass --output-style compressed scss/styles.scss public/styles.css",
    "autoprefixer": "postcss -u autoprefixer -r public/styles.css",
    "cleancss": "cleancss -o public/styles.css public/styles.css",
    "image": "imagemin img/* -o public/img",
    "font": "cp font/*.* public/font/",
    "html": "cp *.html public/",
    "start": "nodemon --harmony server.js",
    "build:html": "npm run html && npm run image && npm run font",
    "build:css": "npm run scss && npm run autoprefixer && npm run cleancss",
    "build:js": "webpack -d",
    "build": "npm run test && npm run build:html && npm run build:css && npm run build:js",
    "watch:model": "onchange \"./models/*.js\" -- npm run test",
    "watch:functions": "onchange \"./programFunctions/*.js\" -- npm run test",
    "watch:test": "onchange \"./test/*.js\" -- npm run test",
    "watch:html": "onchange \"./*.html\" -- npm run build:html",
    "watch:css": "onchange \"./scss/*.scss\" -- npm run build:css",
    "watch:js": "onchange \"./components/*.js\" -- npm run build:js",
    "watch:flow": "onchange \"./flow/*.js\" -- npm run build:js",
    "watch": "concurrently \"npm run watch:test\" \"npm run watch:functions\" \"npm run watch:model\" \"npm run watch:html\" \"npm run watch:css\" \"npm run watch:js\" \"npm run watch:flow\"",
    "begin": "npm run build && npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jstrother/fantasy-soccer-leagues.git"
  },
  "keywords": [
    "fantasy",
    "soccer"
  ],
  "author": "Jim Strother",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jstrother/fantasy-soccer-leagues/issues"
  },
  "homepage": "https://github.com/jstrother/fantasy-soccer-leagues#readme",
  "devDependencies": {
    "autoprefixer": "^7.0.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "clean-css": "^4.1.2",
    "clean-css-cli": "^4.1.2",
    "concurrently": "^3.4.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "imagemin": "^5.2.2",
    "imagemin-cli": "^3.0.0",
    "jest": "^20.0.4",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "normalize-scss": "^6.0.0",
    "onchange": "^3.2.1",
    "postcss-cli": "^4.0.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "material-ui": "^0.17.4",
    "mongodb": "^2.2.26",
    "mongoose": "^4.10.4",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "passport-http-bearer": "^1.0.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "scrape-it": "^3.3.2"
  }
}
